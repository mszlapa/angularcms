{
    "id": "http://cmssite/page-schema#",
    "$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"title": {
			"type": "string"
		},
		"widgets":  {
            "type": "array",
            "minItems": 1,
            "items": { "type": "string" },
            "uniqueItems": true
        }
	},
	"required": ["title", "widgets"]
}